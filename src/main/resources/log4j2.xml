<?xml version="1.0" encoding="UTF-8"?>
<configuration status="OFF">
	<appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %logger{36} %-5level - %msg%n" />
		</Console>
	</appenders>
	<loggers>
		<root level="trace">
			<appender-ref ref="Console" />
		</root>
	</loggers>
</configuration> 